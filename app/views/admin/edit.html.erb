<h1>Edit User</h1>

<%= form_for @user do |f| %>
  <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
</div>

  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div class="actions">
    <%= f.submit "Update Admin" %>
  </div>
<% end %>
